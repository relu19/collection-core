{
  "name": "collection-core",
  "version": "0.0.1",
  "description": "collection core",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16.13.2"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t collection-core .",
    "docker:run": "docker run -p 3000:3000 -d collection-core",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "relu-desktop-2021 <relu1919@yahoo.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^7.0.10",
    "@loopback/build": "^11.0.9",
    "@loopback/core": "^6.1.7",
    "@loopback/eslint-config": "^15.0.5",
    "@loopback/repository": "^7.0.10",
    "@loopback/rest": "^14.0.10",
    "@loopback/rest-explorer": "^7.0.10",
    "@loopback/service-proxy": "^7.0.10",
    "@types/node": "^22.13.0",
    "dotenv": "^16.4.7",
    "loopback-connector-postgresql": "^7.1.9",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/testlab": "^7.0.9",
    "eslint": "^9.19.0",
    "source-map-support": "0.5.21",
    "typescript": "^5.7.3"
  }
}
